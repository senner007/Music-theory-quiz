{
  "name": "music-theory",
  "version": "0.1.0",
  "description": "Harmony based music theory and ear training",
  "main": "main.js",
  "scripts": {
    "dev": "SET NODE_ENV=dev&&nodemon src/main.ts --no-stdin --exitcrash",
    "dev-nocheck": "SET NODE_ENV=dev&&nodemon src/main.ts --no-stdin --config nodemon-transpile-only.json --exitcrash",
    "harm": "npm run dev -- --qt AudiateHarmony --p",
    "build": "tsc",
    "prod": "npm run build && node dist/main.js",
    "test": "vitest --run"
  },
  "keywords": [
    "Ear training",
    "Music theory",
    "Harmony",
    "Solfege",
    "tonal",
    "Typescript"
  ],
  "author": "Niels Theil Gregersen",
  "license": "ISC",
  "dependencies": {
    "@tonaljs/tonal": "^4.8.0",
    "@types/readline-sync": "^1.4.4",
    "ascii-table": "^0.0.9",
    "ascii-table3": "^0.7.8",
    "chalk": "^4.1.2",
    "commander": "^11.0.0",
    "easymidi": "^3.0.1",
    "inquirer": "^8.2.5",
    "inquirer-interrupted-prompt": "^2.0.1",
    "nodemon": "^2.0.15",
    "readline-sync": "^1.4.10",
    "vitest": "^0.32.2"
  },
  "devDependencies": {
    "@type-challenges/utils": "^0.1.1",
    "@types/inquirer": "^9.0.3",
    "@types/node": "^18.11.9",
    "ts-loader": "^9.5.1",
    "ts-node": "^10.9.1",
    "typescript": "^5.0.4",
    "webpack": "^5.90.1",
    "webpack-cli": "^5.1.4"
  },
  "overrides": {
    "vitest": {
      "esbuild": "0.17.5"
    }
  }
}
